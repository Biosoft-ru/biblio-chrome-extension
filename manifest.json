{
  "manifest_version": 2,

  "name": "Biblio chrome extension",
  "version": "0.1.0",
  "description": "",

  "permissions": [
    "storage"
  ],

  "browser_action":{
    "default_popup": "popup/popup.html"
  },

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery.js", "content.js"],
    "matches": ["https://www.ncbi.nlm.nih.gov/pubmed/*", "http://localhost:8000/*"]
  }]

}
